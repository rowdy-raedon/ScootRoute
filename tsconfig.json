{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@store/*": ["store/*"],
      "@types/*": ["types/*"]
    }
  },
  "include": ["src/**/*", "index.js"],
  "exclude": ["node_modules", "android", "ios"]
}